{
  "name": "Mwmbl Web Crawler",
  "description": "Help crawl the web to build a non-profit search engine",
  "version": "0.1",
  "manifest_version": 2,
  "background": {
    "scripts": ["./assets/background.js"],
    "persistent": true
  },
  "permissions": ["storage", "management", "https://*/", "http://*/"],
  "icons": {
    "16": "./mwmbl16.png",
    "32": "./mwmbl32.png",
    "48": "./mwmbl48.png",
    "128": "./mwmbl128.png"
  },
  "options_page": "./options/index.html"
}
