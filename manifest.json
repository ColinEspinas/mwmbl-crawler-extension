{
  "name": "Mwmbl Web Crawler",
  "description": "Help crawl the web to build a non-profit search engine",
  "version": "0.1",
  "manifest_version": 2,
  "background": {
    "scripts": ["./dist/assets/background.js"],
    "persistent": true
  },
  "permissions": ["storage", "management", "https://*/", "http://*/"],
  "icons": {
    "16": "./assets/images/mwmbl16.png",
    "32": "./assets/images/mwmbl32.png",
    "48": "./assets/images/mwmbl48.png",
    "128": "./assets/images/mwmbl128.png"
  },
  "options_page": "./dist/options/index.html"
}
